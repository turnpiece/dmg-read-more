{
  "name": "dmg/read-more",
  "description": "WordPress plugin: DMG Read More",
  "type": "wordpress-plugin",
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "yoast/phpunit-polyfills": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "DMG\\ReadMore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DMG\\ReadMore\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "./setup-wordpress-tests.sh"
    ]
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  }
}
